{
    "name": "danskernesdigitalebibliotek/material-list",
    "description": "Service allowing users to create lists of materials. Based on Lumen. Specificed using OpenAPI.",
    "license": "AGPL",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/danskernesdigitalebibliotek/oauth2-adgangsplatformen"
        }
    ],
    "require": {
        "composer/package-versions-deprecated": "^1.11",
        "danskernesdigitalebibliotek/oauth2-adgangsplatformen": "^1.0",
        "doctrine/dbal": "^3.3",
        "flipbox/lumen-generator": "^9.1",
        "laravel/lumen-framework": "9.0.0",
        "mmghv/lumen-route-binding": "^1.5",
        "platformsh/laravel-bridge": "^2.2",
        "softonic/laravel-psr15-bridge": "^3"
    },
    "require-dev": {
        "behat/behat": "^3.10",
        "ddelnano/dredd-hooks-php": "^1.1",
        "fzaninotto/faker": "^1.4",
        "guzzlehttp/guzzle": "^7.0.1",
        "matthiasnoback/behat-local-code-coverage-extension": "^0.5",
        "mockery/mockery": "^1.0",
        "phpstan/extension-installer": "^1",
        "phpstan/phpstan": "^2",
        "phpstan/phpstan-symfony": "^2",
        "phpunit/phpcov": "^8.2",
        "phpunit/phpunit": "^9.0",
        "slevomat/coding-standard": "^6.4",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "classmap": [
            "database/seeders",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
